{
  "name": "@hierarchidb/ui-layout",
  "version": "1.0.0",
  "type": "module",
  "main": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "development": "./src/index.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build": "tsc -b && tsc-alias",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,scss}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,scss}\""
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@hierarchidb/ui-core": "workspace:*",
    "@hierarchidb/ui-theme": "workspace:*",
    "@mui/material": "^7.3.1",
    "@mui/icons-material": "^7.3.1",
    "react": "^19.1.1"
  },
  "devDependencies": {
    "@types/react": "^19.1.1",
    "typescript": "^5.8.2"
  },
  "license": "MIT"
}

{
  "name": "@hierarchidb/cors-proxy",
  "version": "0.0.1",
  "description": "CORS proxy for external API access",
  "main": "src/index.ts",
  "scripts": {
    "dev:manual": "wrangler dev",
    "build": "tsc --noEmit",
    "deploy": "wrangler deploy",
    "deploy:production": "wrangler deploy --env production",
    "clean": "rm -rf dist build .wrangler",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,scss}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,scss}\""
  },
  "dependencies": {
    "hono": "^4.9.2",
    "jose": "^6.0.12"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250821.0",
    "@types/node": "^24.3.0",
    "wrangler": "^4.31.0"
  },
  "license": "MIT"
}

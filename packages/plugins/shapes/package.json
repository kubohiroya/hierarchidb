{
  "name": "@hierarchidb/plugin-shapes",
  "version": "0.0.1",
  "description": "Shapes plugin for hierarchidb",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc && tsc-alias",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist *.tsbuildinfo",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,scss}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,scss}\""
  },
  "dependencies": {
    "@hierarchidb/core": "workspace:*",
    "@hierarchidb/worker": "workspace:*",
    "react": "^19.0.0",
    "dexie": "^4.0.11"
  },
  "devDependencies": {
    "@types/react": "^18.3.17",
    "fake-indexeddb": "^6.1.0",
    "typescript": "^5.8.2",
    "vitest": "^2.1.9"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0"
  },
  "license": "MIT"
}

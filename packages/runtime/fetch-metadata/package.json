{
  "name": "@hierarchidb/runtime-fetch-metadata",
  "version": "0.0.1",
  "description": "Fetch and transform metadata from various open data sources",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "fetch-metadata": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./output/gadm.json": "./output/gadm.json",
    "./output/geoboundaries.json": "./output/geoboundaries.json",
    "./output/naturalearth.json": "./output/naturalearth.json",
    "./output/osm.json": "./output/osm.json"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "test": "vitest",
    "test:run": "vitest run",
    "fetch:gadm": "node dist/cli.js fetch gadm output gadm.json",
    "fetch:naturalearth": "node dist/cli.js fetch naturalearth output naturalearth.json",
    "fetch:osm": "node dist/cli.js fetch osm output osm.json",
    "fetch:geoboundaries": "node dist/cli.js fetch geoboundaries output geoboundaries.json",
    "fetch": "npm run build ; npm run fetch:gadm ; npm run fetch:naturalearth ; npm run fetch:osm ; npm run fetch:geoboundaries",
    "prefetch:gadm": "npm run build",
    "prefetch:naturalearth": "npm run build",
    "prefetch:osm": "npm run build",
    "prefetch:geoboundaries": "npm run build"
  },
  "dependencies": {
    "commander": "^12.1.0",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@types/commander": "^2.12.5",
    "@types/node": "^20.19.11",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "eslint": "^8.56.0",
    "prettier": "^3.2.5",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3",
    "undici-types": "^7.15.0",
    "vitest": "^1.3.0"
  },
  "files": [
    "output",
    "README.md"
  ],
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "metadata",
    "gadm",
    "osm",
    "open-data",
    "geo",
    "country"
  ],
  "author": "",
  "license": "MIT"
}

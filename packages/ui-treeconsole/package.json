{
  "name": "@hierarchidb/ui-treeconsole",
  "version": "0.1.0",
  "description": "Tree Console UI components for HierarchiDB - migrated from eria-cartograph/app0",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@hierarchidb/core": "workspace:*",
    "@hierarchidb/api": "workspace:*", 
    "@hierarchidb/ui-client": "workspace:*",
    "rxjs": "^7.8.1"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "@mui/material": "^5.15.0",
    "@mui/icons-material": "^5.15.0",
    "@tanstack/react-table": "^8.11.0",
    "@tanstack/react-virtual": "^3.0.0",
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@dnd-kit/utilities": "^3.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "typescript": "^5.3.0",
    "tsup": "^8.0.0",
    "vitest": "^1.2.0",
    "@vitest/ui": "^1.2.0",
    "eslint": "^8.56.0",
    "rimraf": "^5.0.0"
  },
  "keywords": [
    "hierarchidb",
    "tree",
    "console",
    "ui",
    "react",
    "table",
    "virtual",
    "drag-drop"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/hierarchidb.git",
    "directory": "packages/ui-treeconsole"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  }
}
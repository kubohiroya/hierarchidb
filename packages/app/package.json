{
  "name": "@hierarchidb/app",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "src/entry.client.tsx",
  "scripts": {
    "dev": "react-router dev",
    "build": "react-router build --mode production && node scripts/fix-spa-build.js && touch build/client/assets/.nojekyll",
    "build:dev": "react-router build --mode development && node scripts/fix-spa-build.js",
    "build:vite": "vite build --mode production && node scripts/fix-spa-build.js",
    "preview": "vite preview --config vite.preview.config.ts",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,scss}\" \"*.{ts,js,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,scss}\" \"*.{ts,js,json}\"",
    "predeploy": "pnpm run build",
    "deploy": "gh-pages -d build/client -r git@github.com:kubohiroya/hierarchidb.git",
    "test:github-pages": "node scripts/test-github-pages.js",
    "debug:copy": "cp scripts/debug-*.html build/client/ && cp scripts/minimal-*.html build/client/",
    "debug:serve": "npx serve build/client -l 8080"
  },
  "dependencies": {
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@hierarchidb/core": "workspace:*",
    "@hierarchidb/ui-auth": "workspace:*",
    "@hierarchidb/ui-client": "workspace:*",
    "@hierarchidb/ui-core": "workspace:*",
    "@hierarchidb/ui-guide": "workspace:*",
    "@hierarchidb/ui-i18n": "workspace:*",
    "@hierarchidb/ui-import-export": "workspace:*",
    "@hierarchidb/ui-landingpage": "workspace:*",
    "@hierarchidb/ui-layout": "workspace:*",
    "@hierarchidb/ui-navigation": "workspace:*",
    "@hierarchidb/ui-routing": "workspace:*",
    "@hierarchidb/ui-theme": "workspace:*",
    "@hierarchidb/ui-treeconsole-base": "workspace:*",
    "@hierarchidb/ui-treeconsole-toolbar": "workspace:*",
    "@hierarchidb/ui-usermenu": "workspace:*",
    "@mui/icons-material": "^7.3.1",
    "@mui/material": "^7.3.1",
    "@react-router/fs-routes": "^7.8.1",
    "@react-router/node": "^7.8.1",
    "isbot": "^5",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router": "^7.8.1",
    "react-router-dom": "^7.8.1",
    "rxjs": "^7.8.2",
    "vite-plugin-dts": "^4.5.4"
  },
  "devDependencies": {
    "@react-router/dev": "^7.8.1",
    "@react-router/node": "^7.8.1",
    "@types/react": "^19.1.1",
    "@types/react-dom": "^19.1.1",
    "@vitejs/plugin-react": "^4.7.0",
    "gh-pages": "^6.3.0",
    "license-checker": "^25.0.1",
    "typescript": "^5.7.2",
    "vite": "^6.3.5",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "peerDependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "license": "MIT"
}

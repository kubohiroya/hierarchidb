{
  "name": "@hierarchidb/node-type-folder-plugin",
  "version": "1.0.0",
  "description": "Basic folder plugin for HierarchiDB UI layer",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./shared": {
      "types": "./dist/shared/index.d.ts",
      "import": "./dist/shared/index.js",
      "default": "./dist/shared/index.js"
    },
    "./ui": {
      "types": "./dist/ui/index.d.ts",
      "import": "./dist/ui/index.js",
      "default": "./dist/ui/index.js"
    },
    "./worker": {
      "types": "./dist/worker/index.d.ts",
      "import": "./dist/worker/index.js",
      "default": "./dist/worker/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "build:tsc": "tsc",
    "dev:tsc": "tsc --watch"
  },
  "dependencies": {
    "@hierarchidb/common-core": "workspace:*",
    "@hierarchidb/common-api": "workspace:*",
    "@hierarchidb/ui-core": "workspace:*",
    "@hierarchidb/ui-dialog": "workspace:*",
    "dexie": "^4.0.8"
  },
  "peerDependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^5.0.0",
    "@mui/material": "^5.0.0",
    "react": ">=18.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@vitejs/plugin-react": "^5.0.1",
    "fake-indexeddb": "^6.1.0",
    "jsdom": "^26.1.0"
  },
  "keywords": [
    "hierarchidb",
    "plugin",
    "folder",
    "ui"
  ],
  "author": "HierarchiDB Team",
  "license": "MIT",
  "module": "dist/index.js",
  "type": "module"
}

{
  "name": "hierarchidb",
  "private": true,
  "version": "2.0.0",
  "description": "High-performance tree-structured data management framework for browsers",
  "type": "module",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "turbo run dev --parallel",
    "generate-gadm-metadata": "tsx scripts/data-generation/generate-gadm-metadata.ts",
    "generate-naturalearth-metadata": "tsx scripts/data-generation/generate-naturalearth-metadata.ts",
    "generate-openstreetmap-metadata": "tsx scripts/data-generation/generate-openstreetmap-metadata.ts",
    "generate-metadata-index": "tsx scripts/data-generation/generate-metadata-index.ts",
    "generate-gadm-iso-mapping": "tsx scripts/data-generation/generate-gadm-iso-mapping.ts",
    "build": "turbo run build",
    "build:watch": "turbo run build --parallel -- --watch",
    "build:app": "turbo run build --filter=@hierarchidb/app",
    "preview": "turbo run preview",
    "typecheck": "turbo run typecheck",
    "typecheck:watch": "turbo run typecheck --parallel -- --watch",
    "test": "turbo run test --parallel",
    "test:run": "turbo run test:run",
    "e2e": "turbo run e2e",
    "lint": "turbo run lint",
    "format": "turbo run format",
    "clean": "turbo run clean && rm -rf node_modules",
    "storybook:ui-core": "pnpm --filter @hierarchidb/ui-core storybook",
    "check:licenses": "npx license-checker --production --summary",
    "analyze:docs": "node scripts/analyze-docs.js",
    "generate:pdf": "node scripts/generate-pdf-with-translation.js",
    "generate:pdf:ja": "node scripts/generate-pdf-with-translation.js --japanese",
    "generate:pdf:en": "node scripts/generate-pdf-with-translation.js --english",
    "docs:serve": "npx http-server docs/pdf -p 8080",
    "validate:exports": "node scripts/validate-package-exports.cjs",
    "fix:exports": "node scripts/fix-all-package-exports.cjs",
    "count:lines": "node scripts/count-lines.js",
    "analyze:ui": "node scripts/analyze-ui-duplication.js"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@eslint/js": "^9.18.0",
    "@playwright/test": "^1.49.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jscodeshift": "^17.3.0",
    "@types/node": "^18.19.0",
    "@types/react": "^18.3.17",
    "@types/react-dom": "^18.3.5",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "fake-indexeddb": "^6.0.0",
    "glob": "^11.0.3",
    "globals": "^15.14.0",
    "happy-dom": "^16.5.4",
    "http-server": "^14.1.1",
    "jscodeshift": "^17.3.0",
    "license-checker": "^25.0.1",
    "prettier": "^3.4.2",
    "tsc-alias": "^1.8.10",
    "tsup": "^8.5.0",
    "turbo": "^2.3.3",
    "typescript": "4.9.5",
    "vite": "^6.0.7",
    "vite-plugin-checker": "^0.8.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "packageManager": "pnpm@9.14.4",
  "pnpm": {
    "overrides": {
      "@types/react": "^18.3.24",
      "@types/react-dom": "^18.3.7",
      "react-is": "^18.3.1"
    }
  },
  "keywords": [
    "hierarchical-data",
    "tree-structure",
    "indexeddb",
    "browser-database",
    "web-worker",
    "typescript",
    "react"
  ],
  "author": "HierarchiDB Contributors",
  "license": "MIT"
}
